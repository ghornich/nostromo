    "@types/lodash.isequal": "^4.5.5",
    "@types/node": "^22.10.6",
    "@types/puppeteer": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-jest": "^26.5.0",
    "browserify": "^14.3.0",
